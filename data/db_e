SELECT departments.dept_name, departments.dept_no,
dept_manager.emp_no, employees.first_name,
employees.last_name, salaries.from_date,
salaries.to_date

FROM 


SELECT count(*)
FROM departments
LIMIT 5

SELECT *
FROM dept_emp
LIMIT 5

